    "eslint": "8.4.1",
    "eslint-config-next": "12.0.7",
    "husky": "^7.0.0",
    "lint-staged": "^12.1.2",
    "prettier": "^2.5.1",
    "typescript": "4.5.4"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
